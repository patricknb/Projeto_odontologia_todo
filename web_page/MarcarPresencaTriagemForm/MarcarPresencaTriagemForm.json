      "value" : [
        "if( $data.paciente && $data.agendamento ){",
        "\treturn {",
        "\t\t//map paciente variable to expected task contract input",
        "\t\tpacienteInput: {",
        "\t\t\tcpf: $data.paciente.cpf !== undefined ? $data.paciente.cpf : null,",
        "\t\t\tnome: $data.paciente.nome !== undefined ? $data.paciente.nome : null,",
        "\t\t\tprefHorario: $data.paciente.prefHorario !== undefined ? $data.paciente.prefHorario : null",
        "\t\t},",
        "\t\t//map agendamento variable to expected task contract input",
        "\t\tagendamentoInput: {",
        "\t\t\tpresencaTriagem: $data.agendamento.presencaTriagem !== undefined ? $data.agendamento.presencaTriagem : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
